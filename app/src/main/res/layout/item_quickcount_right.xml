<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/ll_quickCount3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:gravity="center"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:id="@+id/tv_quickCount2"
        android:layout_weight="1"
        android:textColor="@color/putih"
        android:layout_marginBottom="@dimen/margin8dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textSize="@dimen/text32dp"
        android:text="@string/quick_nomor2"
        android:layout_gravity="center"
        android:gravity="center"/>

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/iv_candidate2"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="@dimen/margin128dp"
        android:layout_height="@dimen/margin128dp"
        android:layout_margin="@dimen/margin8dp"
        android:src="@mipmap/ic_launcher_round"
        android:elevation="@dimen/margin4dp"/>


</LinearLayout>